{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": ["airbnb-base"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "arrow-body-style": "off",
      "object-curly-newline": [
        "error",
        {
          "multiline": true
        }
      ]
    }
}
